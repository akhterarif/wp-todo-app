{
    "name": "wp-todo-app",
    "version": "1.0.0",
    "description": "WordPress Todo App with Optimistic UI",
    "main": "src/index.js",
    "scripts": {
        "check-engines": "wp-scripts check-engines",
        "check-licenses": "wp-scripts check-licenses",
        "build": "npx @tailwindcss/cli -i ./src/tailwind.css -o ./dist/css/tailwind.css --watch && wp-scripts build",
        "start": "npx @tailwindcss/cli -i ./src/tailwind.css -o ./dist/css/tailwind.css --minify && wp-scripts start",
        "format": "wp-scripts format",
        "lint:css": "wp-scripts lint-style",
        "lint:js": "wp-scripts lint-js",
        "lint:md:docs": "wp-scripts lint-md-docs",
        "lint:pkg-json": "wp-scripts lint-pkg-json",
        "test:e2e": "wp-scripts test-e2e",
        "test:unit": "wp-scripts test-unit-js"
    },
    "dependencies": {
        "@tailwindcss/cli": "^4.1.1",
        "@wordpress/api-fetch": "^7.21.0",
        "@wordpress/components": "^27.0.0",
        "@wordpress/data": "^9.0.0",
        "@wordpress/element": "^6.0.0",
        "@wordpress/i18n": "^5.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@wordpress/scripts": "^27.9.0",
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.3",
        "postcss-import": "^16.1.0",
        "tailwindcss": "^4.1.1"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ],
    "resolutions": {
        "react": "18.2.0",
        "react-dom": "18.2.0"
    }
}